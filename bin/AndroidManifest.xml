<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.corso"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.corso.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.corso.SecondaActivity"
            android:label="@string/title_activity_seconda" >
        </activity>
        <activity
            android:name="com.example.corso.ListaActivity"
            android:label="@string/title_activity_lista" >
        </activity>
        <activity
            android:name="com.example.corso.DettaglioActivity"
            android:label="@string/title_activity_dettaglio" >
        </activity>
        <activity
            android:name="com.example.corso.DettaglioWebViewActivity"
            android:label="@string/title_activity_dettaglio_web_view"
            android:parentActivityName="com.example.corso.ListaActivity" >
        </activity>
        <activity
            android:name="com.example.corso.ListaPersoneActivity"
            android:label="@string/title_activity_lista_persone" >
        </activity>
        <activity
            android:name="com.example.corso.CreazioneModificaActivity"
            android:label="@string/title_activity_creazione_modifica" >
        </activity>
        <activity
            android:name="com.example.corso.DettaglioContattoActivity"
            android:label="@string/title_activity_dettaglio_contatto" >
        </activity>
    </application>

</manifest>
